<?xml version="1.0" encoding="utf-8"?>
<!--
状态的美化：state-container标签
组件的状态：item

默认状态必须要放在所有状态的最后面

-->
<state-container
    xmlns:ohos="http://schemas.huawei.com/res/ohos">

    <!--按下去的状态-->
    <item
        ohos:element="$graphic:ability_bf_shape_02_01"
        ohos:state="component_state_pressed"/>
    <!--
默认状态
必须要放在所有状态的最后面
-->
    <item
        ohos:element="$graphic:ability_bf_shape_02_02"
        ohos:state="component_state_empty"/>


</state-container>
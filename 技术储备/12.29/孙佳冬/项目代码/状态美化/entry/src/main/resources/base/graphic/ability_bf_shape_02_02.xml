<?xml version="1.0" encoding="utf-8"?>
<!--
shape
    属性  shape   组件的外形
         oval  椭圆
         rectangle  长方形
   子标签：stroke 设置组件的边框
         color：边框的颜色
         width: 边框的宽度
   子标签：corners
         设置组件的圆角
   子标签：solid
         设置组件的背景颜色
   子标签：gradient
         设置组件颜色渐变

-->
<shape
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:shape="rectangle">

    <stroke ohos:color="#CCCCCC"
            ohos:width="5vp">
    </stroke>
    <corners
        ohos:radius="15vp"/>
    <solid ohos:colors="red"/>

</shape>